@{
    ViewData["Title"] = "Home Page";

}
@ViewBag.Orders;
<style>
    .loan-results {
        height: 3em;
        font-size: 1.5em;
        color: #3C3;
        background-color: #CCC;
    }
</style>
<div class="col-md-12 col-lg-12">
    <div class="container">
        @foreach (var item in ViewBag.Orders)
        {
            <div class="col-md-4 loan-results">
                <div>@item.ClientLName, @item.ClientFName</div>
                <div> Loan Amount: @item.LoanTotal.ToString("C2")</div>
            </div>

        }
    </div>
</div>
